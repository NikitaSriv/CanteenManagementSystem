30.TO INCREASE PRICE OF EVERY BOOK BY 10% WHOSE PRICE IS LESS THAN 300
update book set price=price*0.1 where price<300;

31.TO DELETE ALL RECORDS FROM TABLE BOOK WHOSE PRICE IS 0 TO 300
delete from b where price BETWEEN 0 and 300;

32.TO DROP COLUMN ISBN_NO FROM TABLE BOOK
alter table book drop column isbn_no;

33.QUERY TO DROP TABLE BOOK

DROP TABLE book;


